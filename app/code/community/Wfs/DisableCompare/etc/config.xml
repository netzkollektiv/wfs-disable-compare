<config>
    <modules>
        <Wfs_DisableCompare>
            <version>0.0.1</version>
        </Wfs_DisableCompare>
    </modules>
    <global>
        <models>
            <wfs_disablecompare>
                <class>Wfs_DisableCompare_Model</class>
            </wfs_disablecompare>
        </models>
        <helpers>
            <wfs_disablecompare>
                <class>Wfs_DisableCompare_Helper</class>
            </wfs_disablecompare>
            <catalog>
                <rewrite>
                    <product_compare>Wfs_DisableCompare_Helper_Compare</product_compare>
                </rewrite>
            </catalog>
        </helpers>
    </global>
    <frontend>
        <layout>
            <updates>
                <wfs_disablecompare>
                    <file>wfs/disablecompare.xml</file>
                </wfs_disablecompare>
            </updates>
        </layout>
    </frontend>
</config>